{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/fa-brands": "^10.38.0",
    "@styled-icons/material": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/material-rounded": "^10.34.0",
    "@styled-icons/material-sharp": "^10.34.0",
    "@styled-icons/material-twotone": "^10.34.0",
    "@types/lodash.xor": "^4.5.6",
    "add": "^2.0.6",
    "lodash.xor": "^4.5.0",
    "next": "12.0.10",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2",
    "yarn": "^1.22.18"
  },
  "resolutions": {
    "graphql": "^15.5.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "apollo": "^2.33.10",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  }
}
